<name>!lib.drone.check.is.space.docktarget</name>
<version>1</version>
<engineversion>72</engineversion>
<description>Checks drone dock target has enough space in cargo bay for drone ware.</description>
<arguments>
<argument index="1" name="target" type="Var/Ship/Station" desc="" />
</arguments>
<sourceplaintext>
<![CDATA[
  skip if $target -> is of class Ship
    return [TRUE]
  
  $ware = [THIS] -> get ware type code of object
  $ware = get catch ware type: ware=$ware
  do if $target -> get free amount of ware $ware in cargo bay
    return [TRUE]
  
  if [OWNER] == Player
    $text = sprintf: pageid=[Page.MiscVoice] textid=32, null, null, null, null, null
    display notification: message=$text, icon=104
    $speak = create new array, arguments=[Page.MiscVoice], 135, [Page.MiscVoice], 32, null
    START speak array: $speak prio=60
  end
  
  return [FALSE]
]]>
</sourceplaintext>
