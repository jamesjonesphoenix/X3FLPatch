<script>
<name>!ship.cmd.returnhome.std</name>
<version>3</version>
<engineversion>72</engineversion>
<description>Ship Standard COMMAND RETURN HOME</description>
<arguments>
</arguments>
<sourceplaintext>
<![CDATA[
  [THIS] ->set command: COMMAND_RETURN_HOME  target=null target2=null par1=null par2=null
  
  if not [HOMEBASE] -> exists
    do if [OWNER] == Player
      START speak text: page=[Page.MiscVoice] id=135 priority=60
    return FLRET_INVALIDPARMS
  end
  
  if [THIS] -> is of class Drone
@   skip if [THIS] -> call script !lib.drone.check.is.space.docktarget :  =[HOMEBASE]
      return FLRET_NOCARGO
@   = wait 1000 ms
  else
    if not [HOMEBASE] -> is docking possible of [THIS]
      do if [OWNER] == Player
        START speak text: page=[Page.MiscVoice] id=135 priority=60
      return FLRET_CANTDOCK
    end
  end
  
  [THIS] ->set destination to [HOMEBASE]
@ $ret = [THIS] -> call script !move.returntohomebase : 
  if $ret == FLRET_NOPATH
    if [OWNER] == Player
      $home.sector = [HOMEBASE] -> get sector
@     = [THIS] -> call script !lib.player.cmd.moverejected :  Destination=$home.sector
    end
  end
  
  [THIS] ->set destination to null
  return $ret
]]>
</sourceplaintext>
