<script>
<name>!lib.freight.transfer.or.eject</name>
<version>1</version>
<engineversion>72</engineversion>
<description>Transfer cargo from drone to ship. Eject any wares that won't fit into space as flying wares</description>
<arguments>
<argument index="1" name="wares" type="Var/Table" desc="" />
</arguments>
<sourceplaintext>
<![CDATA[
  if [OWNER] == Player
    $debug = 9595
    $id = [THIS] -> get ID code
    $prepend = sprintf: fmt='Freight transfer to "%s" -', [THIS], null, null, null, null
    write to log file #$debug  append=[TRUE]  printf: fmt='%s Wares in drone', $prepend, null, null, null, null
@   = [THIS] -> call script debug.printdata.table :  Data Table=$wares  File ID=$debug  Tab=''
  end
  
  $x = [THIS] -> get x position
  $y = [THIS] -> get y position
  $z = [THIS] -> get z position
  
  $ware = get next key: table=$wares key=null
  while $ware
@   = wait 50 ms
    $amount = $wares[$ware]
    if not $amount
      $ware = get next key: table=$wares key=$ware
      continue
    end
    
    if [THIS] -> get free amount of ware $ware in cargo bay
      $added = [THIS] -> add $amount units of $ware
      do if [OWNER] == Player
        write to log file #$debug  append=[TRUE]  printf: fmt='%s Transferred %s units of %s', $prepend, $added, $ware, null, null
    else
      $added = 0
    end
    
    if $added != $amount
  * Eject any wares that did not fit target cargo bay into space as flying wares. 
      $amount = $amount - $added
@     = [THIS] -> call script !galaxyeditor.create.flyingware :  ware=$ware  count=$amount  x=$x  y=$y  z=$z
      
      do if [OWNER] == Player
        write to log file #$debug  append=[TRUE]  printf: fmt='%s Created flying ware with %s units of %s', $prepend, $amount, $ware, null, null
    end
    $ware = get next key: table=$wares key=$ware
  end
  
  return null
]]>
</sourceplaintext>
