<script>
<name>!ship.cmd.movestation.std</name>
<version>4</version>
<engineversion>72</engineversion>
<description>Ship Standard COMMAND DOCKAT</description>
<arguments>
<argument index="1" name="object" type="Var/Station/Carrier to dock at" desc="object to land on" />
</arguments>
<sourceplaintext>
<![CDATA[
  $sector = $object -> get sector
@ skip if [THIS] -> call script !lib.player.checkvalidroute :  Destination Sector=$sector  Allow Target=[TRUE]  Command Rejected Sound=[TRUE]
    return FLRET_NOPATH
  
  if [THIS] -> is of class Drone
@   skip if [THIS] -> call script !lib.drone.check.is.space.docktarget :  =$object
      return FLRET_NOCARGO
  end
  
  [THIS] ->set command: COMMAND_DOCKAT  target=$object target2=null par1=null par2=null
  
  [THIS] ->set destination to $object
@ $ret = [THIS] -> call script !move.movetostation :  station=$object
  if $ret == FLRET_NOPATH
    $sector = $object -> get sector
@   = [THIS] -> call script !lib.player.cmd.moverejected :  Destination=$sector
  end
  
  [THIS] ->set destination to null
  return $ret
]]>
</sourceplaintext>
